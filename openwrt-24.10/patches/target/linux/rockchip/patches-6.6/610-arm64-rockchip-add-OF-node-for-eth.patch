 	phy-handle = <&rgmii_phy1>;
 	phy-mode = "rgmii-id";
 	pinctrl-names = "default";
@@ -76,6 +78,7 @@
 		reg = <0x1>;
 		pinctrl-0 = <&eth_phy0_reset_pin>;
 		pinctrl-names = "default";
+		realtek,led-data = <0x6d60>;
 	};
 };
 
@@ -85,6 +88,7 @@
 		reg = <0x1>;
 		pinctrl-0 = <&eth_phy1_reset_pin>;
 		pinctrl-names = "default";
+		realtek,led-data = <0x6d60>;
 	};
 };
 
@@ -106,6 +110,14 @@
 	vccio3-supply = <&vcc_3v3>;
 };
 
+&rtl8125_1 {
+	label = "eth3";
+};
+
+&rtl8125_2 {
+	label = "eth2";
+};
+
 &sdhci {
 	bus-width = <8>;
 	max-frequency = <200000000>;
